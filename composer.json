{
  "name": "prokerala/astrology-sdk",
  "description": "Prokerala.com Astrology API Client Library for PHP.",
  "type": "library",
  "version": "0.4",
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/simple-cache": "^1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Prokerala",
      "email": "support@prokerala.com",
      "homepage": "https://www.prokerala.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Prokerala\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Prokerala\\test\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15"
  }
}
